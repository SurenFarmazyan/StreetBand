<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomMainBoard">
        <attr name="board_length" format="float"/>
    </declare-styleable>
    <declare-styleable name="CustomEditBoard">
        <attr name="start" format="float"/>
        <attr name="song_length" format="float"/>
        <attr name="octave_sum" format="integer"/>
    </declare-styleable>
    <declare-styleable name="CustomSeekBar">
        <attr name="tact">
            <flag name="three" value="1"/>
            <flag name="four" value="2"/>
            <flag name="six" value="4"/>
        </attr>
        <attr name="length" format="integer"/>
    </declare-styleable>
    <declare-styleable name="CustomCursor">
        <attr name="position" format="float"/>
        <attr name="show_line" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="CustomNavigationDrawer">
        <attr name="drawerEndX" format="dimension"/>
        <attr name="shadowRadius" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="CustomNumberController">
        <attr name="max_number" format="integer"/>
        <attr name="min_number" format="integer"/>
        <attr name="up_arrow" format="reference"/>
        <attr name="down_arrow" format="reference"/>
    </declare-styleable>
</resources>